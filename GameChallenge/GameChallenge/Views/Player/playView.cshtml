
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.1/js/materialize.min.js"></script>





<div>
    <div class="row">
        <!--Dentro de esta etiqueta va el contenido-->

        <h3 class="orange-text center-align">Let' s play</h3>
       
        @using (Html.BeginForm("Result", "Player", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="col s10 offset-s1">
        <div class="row">
            <div class="input-field col s12">
                <div class="file-field input-field">
                    <div class="btn">
                        <span>File</span>
                        <input type="file" name="file" id="file">
                    </div>
                    <div class="file-path-wrapper">
                        <input class="file-path validate" type="text">
                    </div>
                </div>

            </div>
        </div>

        <div class="row">
            <div class="center-align">
                <input type="submit" value="Upload and Play" class="btn btn-info" />

            </div>
        </div>
    </div>

}
            

       

    </div>

</div>
